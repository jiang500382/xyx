button {
    color: inherit;
}

button:after {
    border: none;
}

.container {
    height: 100%;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
    -webkit-box-align: center;
    align-items: center;
    -webkit-box-pack: justify;
    justify-content: space-between;
    box-sizing: border-box;
}

.drowPoput {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,0.7);
    z-index: 199;
}

.darkBrown {
    color: #a83e05;
}

.overlips {
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}