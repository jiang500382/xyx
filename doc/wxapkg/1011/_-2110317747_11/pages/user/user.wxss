page {
    height: 100%;
}

.container {
    min-height: 100%;
    position: relative;
}

.bottom_img {
    position: absolute;
    bottom: 0;
    height: 160rpx;
    width: 100%;
    z-index: -1;
}

.bottom_img image {
    width: 100%;
    height: 100%;
    display: block;
}

.userInfo {
    margin: 0 30rpx 30rpx;
    padding: 100rpx 30rpx 30rpx;
    box-sizing: border-box;
    background-color: #fff;
    border-radius: 20rpx;
    position: relative;
}

.userInfo .logo {
    width: 250rpx;
    height: 164rpx;
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
    top: -80rpx;
    text-align: center;
    background: url("https://txcdn.ylll111.xyz/hzttk/userlogo-bg.png");
    background-size: cover;
    padding: 0;
}

.userInfo .logo::after {
    border: none;
}

.userInfo .logo image {
    width: 130rpx;
    height: 130rpx;
    border-radius: 50%;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    margin-left: 4rpx;
}

.userNum .list {
    width: 33.3%;
    text-align: center;
    margin-bottom: 40rpx;
    font-size: 28rpx;
    color: #666;
}

.userNum .list-black {
    width: 50%;
}

.userNum .list .num {
    margin-top: 30rpx;
    font-size: 24rpx;
    color: #664a2c;
}

.userNum .list .num text {
    font-size: 54rpx;
    font-weight: 600;
    margin-right: 4rpx;
}

.userNum .list image {
    width: 40rpx;
    height: 40rpx;
    margin-right: 4rpx;
    vertical-align: top;
}

.tixian {
    width: 180rpx;
    height: 78rpx;
    line-height: 78rpx;
    text-align: center;
    border-radius: 60rpx;
    background-color: #cd4e3d;
    font-size: 30rpx;
    color: #FFF;
    float: left;
}

.kfbtn {
    width: 416rpx;
    height: 78rpx;
    line-height: 78rpx;
    text-align: center;
    border-radius: 60rpx;
    background-color: #22ac38;
    float: right;
    font-size: 30rpx;
    color: #FFF;
    padding: 0;
}

.kfbtn .icon {
    width: 40rpx;
    height: 36rpx;
    margin-right: 10rpx;
    vertical-align: middle;
}

.record {
    margin: 0 30rpx 30rpx;
    border-radius: 20rpx;
    background-color: #fff;
    height: 460rpx;
    overflow: hidden;
}

.record .title {
    height: 94rpx;
    line-height: 94rpx;
    text-align: center;
    font-size: 32rpx;
    color: #664a2c;
    font-weight: 600;
}

.record .norecord {
    border-top: 2rpx solid #eee;
    box-sizing: border-box;
    padding-top: 50rpx;
}

.record .norecord image {
    width: 142rpx;
    height: 174rpx;
    display: block;
    margin: 0 auto 20rpx;
}

.record .norecord .txt {
    font-size: 32rpx;
    color: #acacac;
    text-align: center;
}

.record .listBox .title {
    height: 74rpx;
    line-height: 74rpx;
    background-color: #f2f2f2;
    padding: 0 50rpx;
    box-sizing: border-box;
    font-size: 24rpx;
    color: #666;
}

.record .listBox .title .fr {
    text-align: center;
    width: 280rpx;
}

.record .listBox .con {
    overflow: auto;
    height: 292rpx;
}

.record .listBox .con .list {
    height: 74rpx;
    line-height: 74rpx;
    padding: 0 50rpx;
    box-sizing: border-box;
    font-size: 28rpx;
    color: #666;
    border-bottom: 2rpx solid #eee;
}

.game {
    padding: 20rpx;
    border-radius: 20rpx;
    background-color: #fff;
    margin: 0 30rpx 30rpx;
}

.game .logo {
    width: 108rpx;
    height: 108rpx;
    border-radius: 50%;
    background-color: #999;
    float: left;
    margin-right: 20rpx;
}

.game .logo image {
    width: 100%;
    height: 100%;
    display: block;
    border-radius: 50%;
}

.game .go {
    float: right;
    width: 120rpx;
    height: 50rpx;
    border-radius: 6rpx;
    text-align: center;
    line-height: 50rpx;
    font-size: 28rpx;
    color: #fff;
    background-color: #8d7860;
    margin-top: 30rpx;
}

.game .con {
    float: left;
    width: calc(100% - 260rpx);
}

.game .con .tit {
    font-size: 34rpx;
    margin-top: 10rpx;
    margin-bottom: 10rpx;
    color: #333;
}

.game .con .txt {
    font-size: 24rpx;
    color: #999;
}

.moreGame .game:last-child {
    margin-bottom: 0;
}

.complaint {
    position: fixed;
    top: 50rpx;
    right: 0;
}

.complaint button {
    display: flex;
    flex-flow: row;
    align-items: center;
    justify-content: center;
    margin: 0;
    padding: 0;
    width: 140rpx;
    height: 64rpx;
    line-height: 64rpx;
    background: #664a2c;
    border: none;
    border-top-left-radius: 36rpx;
    border-top-right-radius: 0;
    border-bottom-left-radius: 36rpx;
    border-bottom-right-radius: 0;
    color: #fff;
    font-size: 28rpx;
}

.complaint .icon {
    margin-right: 10rpx;
    width: 40rpx;
    height: 40rpx;
}

.shareBox {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,0.6);
}

.shareBox .bg {
    width: 500rpx;
    height: 600rpx;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    background-size: cover;
    background-image: url('data:image/png;base64,');
    padding: 50rpx 0;
    box-sizing: border-box;
}

.shareBox .txt {
    font-size: 40rpx;
    color: #fff;
    text-align: center;
}

.shareBox .btn {
    width: 400rpx;
    height: 76rpx;
    margin: 0 auto;
    text-align: center;
    border-radius: 40rpx;
    color: #fff;
    font-size: 36rpx;
    background: #e14e4c;
    position: absolute;
    bottom: 50rpx;
    left: 50%;
    line-height: 76rpx;
    margin-left: -200rpx;
}

.shareBox .close {
    position: absolute;
    width: 60rpx;
    height: 60rpx;
    right: -26rpx;
    top: -26rpx;
    display: block;
}