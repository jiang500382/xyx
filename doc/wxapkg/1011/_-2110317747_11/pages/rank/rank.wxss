page {
    height: 100%;
    position: relative;
}

.bottom_img {
    position: absolute;
    bottom: 0;
    height: 160rpx;
    width: 100%;
}

.bottom_img image {
    width: 100%;
    height: 100%;
    display: block;
}

.swiper-tab {
    margin: 0 30rpx 30rpx;
    padding-top: 30rpx;
    box-sizing: border-box;
}

.swiper-tab .tab-list {
    display: inline-block;
    width: calc(50% - 60rpx);
    margin: 0 30rpx;
    height: 74rpx;
    line-height: 74rpx;
    font-size: 28rpx;
    color: #666;
    background: #dcd6d0;
    text-align: center;
    border-radius: 40rpx;
}

.swiper-tab .tab-list.on {
    background: #333333;
    color: #fff;
}

.swiper-box {
    height: calc(100% - 144rpx);
}

.swiper-container {
    height: 100%;
}

.swiper-bottom {
    display: flex;
    align-items: center;
    justify-content: center;
    height: 130rpx;
}

.swiper-bottom text {
    display: inline-block;
    padding: 0 50rpx;
    width: auto;
    height: 72rpx;
    line-height: 72rpx;
    background: #7401aa;
    border-radius: 36rpx;
    color: #fff;
    text-align: center;
}

.rank-list {
    padding: 0 30rpx;
}

.rank-item {
    position: relative;
    margin-top: 10rpx;
    padding: 20rpx 30rpx;
    height: 80rpx;
    background: #f7f5f2;
    color: #fff;
    border-radius: 10rpx;
}

.rank-item:first-child:after {
    content: "";
    position: absolute;
    left: 70rpx;
    top: -5rpx;
    width: 72rpx;
    height: 43rpx;
    background: url(data:image/png;base64,);
    background-repeat: no-repeat;
    background-position: 0 0;
    background-size: cover;
}

.rank-item .num {
    float: left;
    line-height: 80rpx;
    text-align: center;
    font-size: 36rpx;
    color: #000;
}

.rank-item .avatar {
    float: left;
    margin-left: 30rpx;
    width: 80rpx;
    height: 80rpx;
    border-radius: 100%;
}

.rank-item .name {
    float: left;
    margin-left: 30rpx;
    width: 290rpx;
    line-height: 80rpx;
    font-size: 28rpx;
    color: #333;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}

.rank-item .gift {
    float: right;
    line-height: 80rpx;
    font-size: 24rpx;
    color: #ce503e;
    font-weight: 700;
}

.rank-item .gift text {
    margin: 0 5rpx;
    font-weight: 700;
    font-size: 56rpx;
}