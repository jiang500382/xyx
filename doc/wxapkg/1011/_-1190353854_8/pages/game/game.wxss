page {
    height: 100%;
    position: relative;
}

.bottom_img {
    position: absolute;
    bottom: 0;
    height: 160rpx;
    width: 100%;
}

.bottom_img image {
    width: 100%;
    height: 100%;
    display: block;
}

.tips {
    height: 86rpx;
    line-height: 86rpx;
    text-align: center;
    background-color: #e0dad5;
    font-size: 28rpx;
    color: #333;
    position: relative;
}

.tips .animg,.tips .animg_off {
    position: absolute;
    width: 54rpx;
    height: 54rpx;
    top: 50%;
    margin-top: -25rpx;
    right: 30rpx;
}

.tips .animg image,.tips .animg_off image {
    width: 100%;
    height: 100%;
}

.tips .animg {
    animation: musicAnima 2s linear infinite;
}

@-webkit-keyframes musicAnima {
    0% {
        transform: rotate(0deg);
    }

    100% {
        transform: rotate(360deg);
    }
}

@keyframes musicAnima {
    0% {
        transform: rotate(0deg);
    }

    100% {
        transform: rotate(360deg);
    }
}

.bar {
    position: relative;
    width: 640rpx;
    height: 44rpx;
    background-color: rgba(255,255,255,0.6);
    border-radius: 20rpx;
    margin: 34rpx auto 34rpx;
    overflow: hidden;
}

.bar1 {
    position: absolute;
    left: 6rpx;
    top: 6rpx;
    right: 6rpx;
    bottom: 6rpx;
    background: #434343;
    border-radius: 20rpx;
}

.bar2 {
    position: absolute;
    left: 6rpx;
    top: 6rpx;
    right: 6rpx;
    bottom: 6rpx;
    max-width: calc(100% - 12rpx);
    background: linear-gradient(to right,#ef75b2,#dd76ac,#5d96dd);
    border-radius: 20rpx;
    overflow: hidden;
}

.bar2.play {
    width: 100%;
    animation-name: bar-play;
    animation-timing-function: linear;
}

.bar2.replay {
    width: 100%;
    animation-name: bar-replay;
    animation-timing-function: linear;
}

.bar2.stop {
    animation-play-state: paused;
}

.bar2.over {
    width: 0;
}

@-webkit-keyframes bar-play {
    0% {
        width: 100%;
    }

    100% {
        width: 0;
    }
}

@keyframes bar-play {
    0% {
        width: 100%;
    }

    100% {
        width: 0;
    }
}

@-webkit-keyframes bar-replay {
    0% {
        width: 100%;
    }

    100% {
        width: 0;
    }
}

@keyframes bar-replay {
    0% {
        width: 100%;
    }

    100% {
        width: 0;
    }
}

.bar3 {
    position: absolute;
    left: 6rpx;
    top: 6rpx;
    right: 6rpx;
    bottom: 6rpx;
    border-radius: 20rpx;
    background-repeat: no-repeat;
    background-position: 0 0;
    background-size: cover;
    background-image: url('data:image/png;base64,');
}

.score {
    height: 130rpx;
    font-size: 44rpx;
    color: #333;
    font-weight: 600;
    line-height: 100rpx;
    margin-bottom: 60rpx;
    text-align: center;
    background-repeat: no-repeat;
    background-position: center center;
    background-size: 366rpx 130rpx;
    background-image: url('data:image/png;base64,');
}

.question {
    font-size: 100rpx;
    color: #333;
    text-align: center;
}

.question .question-title {
    height: 110rpx;
    width: 110rpx;
    font-size: 100rpx;
    display: inline-block;
}

.question .question-title2 {
    height: 110rpx;
    width: 95rpx;
    font-size: 80rpx!important;
    display: inline-block;
}

.question .question-title-active {
    font-weight: bold;
    color: #c4523f;
    text-align: center;
    line-height: 148rpx;
    position: relative;
}

.select_ans {
    padding: 0 64rpx;
    margin-top: 110rpx;
}

.select_ans .fl,.select_ans .fr {
    width: 280rpx;
    height: 280rpx;
    border-radius: 50%;
    border: 4rpx solid #333;
    background-size: cover;
    font-size: 72rpx;
    font-weight: bold;
    color: #333;
    text-align: center;
    line-height: 280rpx;
    position: relative;
}

.select_ans .fl.right,.select_ans .fr.right {
    color: #393;
    border: 4rpx solid #393;
}

.select_ans .fl.wrong,.select_ans .fr.wrong {
    color: #ce503e;
    border: 4rpx solid #ce503e;
}

.select_ans .fl.right::after,.select_ans .fr.right::after {
    content: "";
    position: absolute;
    background-image: url('data:image/png;base64,');
    width: 196rpx;
    height: 140rpx;
    left: 50%;
    margin-left: -98rpx;
    bottom: -100rpx;
    background-size: cover;
}

.select_ans .fl.wrong::after,.select_ans .fr.wrong::after {
    content: "";
    position: absolute;
    background-image: url('data:image/png;base64,');
    width: 154rpx;
    height: 154rpx;
    background-size: cover;
    left: 50%;
    margin-left: -77rpx;
    bottom: -100rpx;
}

.popup-win {
    position: fixed;
    left: 0;
    width: 100%;
    height: 100%;
    top: 0;
    z-index: 1000;
    background: rgba(0,0,0,0.6);
}

.popup-win .popup-panel {
    width: 604rpx;
    height: 784rpx;
    position: absolute;
    top: 40%;
    left: 50%;
    transform: translate(-50%,-50%);
    background-image: url(data:image/png;base64,);
    background-repeat: no-repeat;
    background-size: 100% 100%;
}

.popup-win .popup-main {
    padding: 206rpx 46rpx 40rpx;
    text-align: center;
}

.popup-win .popup-main .image {
    width: 200rpx;
    height: 200rpx;
    margin-top: 10rpx;
    margin-left: 10rpx;
}

.popup-win .popup-main .title {
    width: 144rpx;
    height: 144rpx;
    border-radius: 50%;
    font-size: 44rpx;
    text-align: center;
    color: #fff;
    position: absolute;
    top: 270rpx;
    left: 50%;
    transform: translate(-50%);
}

.popup-win .popup-main .message {
    text-align: center;
    line-height: 56rpx;
    font-size: 44rpx;
    color: #ffe493;
    position: absolute;
    top: 420rpx;
    left: 50%;
    transform: translate(-50%);
    width: 400rpx;
    margin-top: 30rpx;
}

.rotateBtn {
    animation: rotateBtn 2s;
}

@-webkit-keyframes rotateBtn {
    0% {
        transform: rotateY(0deg);
    }

    50% {
        transform: rotateY(360deg);
    }

    100% {
        transform: rotateY(720deg);
    }
}

@keyframes rotateBtn {
    0% {
        transform: rotateY(0deg);
    }

    50% {
        transform: rotateY(360deg);
    }

    100% {
        transform: rotateY(720deg);
    }
}

.popup-win .button-share {
    width: 128rpx;
    height: 128rpx;
    line-height: 128rpx;
    background: #f36;
    border-radius: 50%;
    font-size: 32rpx;
    color: #e62412;
    position: relative;
    bottom: -350rpx;
    border: none;
    background: #f2c948;
    margin: 0 auto;
}

.popup-win .button-share::after {
    border: none;
}

.popup-win .popup-close {
    position: absolute;
    right: 50px;
    top: 150px;
    z-index: 10003;
    width: 30px;
    height: 30px;
}

.popup-fail {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 1000;
    background: rgba(0,0,0,0.6);
}

.popup-fail .popup-panel {
    width: 640rpx;
    box-sizing: border-box;
    background-color: #e1d2b1;
    border: 4rpx solid #664a2c;
    border-radius: 40rpx;
    padding: 100rpx 50rpx 50rpx;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
}

.popup-fail .title {
    font-size: 40rpx;
    color: #5a3e38;
    font-weight: 600;
    margin-bottom: 50rpx;
}

.popup-fail .explain text {
    font-size: 28rpx;
    color: #5a3e38;
    display: block;
    line-height: 44rpx;
}

.popup-fail .fail_share {
    width: 440rpx;
    height: 88rpx;
    border-radius: 60rpx;
    background-color: #664a2c;
    line-height: 88rpx;
    font-size: 36rpx;
    color: #fff;
    margin: 60rpx auto 30rpx;
    box-shadow: 0 6rpx 10rpx 2rpx rgba(0,0,0,0.2);
}

.popup-fail .closeBtn {
    width: 440rpx;
    height: 88rpx;
    line-height: 88rpx;
    box-sizing: border-box;
    border-radius: 60rpx;
    border: 1px solid #664a2c;
    text-align: center;
    color: #664a2c;
    font-size: 36rpx;
    margin: 0 auto;
    background-color: #eee5d2;
    box-shadow: 0 6rpx 10rpx 2rpx rgba(0,0,0,0.2);
}

.popup-fail .close {
    position: absolute;
    width: 60rpx;
    height: 60rpx;
    right: -26rpx;
    top: -26rpx;
}

.popup-fail .close image {
    width: 100%;
    height: 100%;
    display: block;
}

.popup-fail .top_img {
    position: absolute;
    width: 410rpx;
    height: 110rpx;
    left: 50%;
    margin-left: -205rpx;
    top: -44rpx;
}

.popup-fail.ad {
    height: calc(100% - 214rpx);
}

.popup-win.ad {
    height: calc(100% - 214rpx);
}

.banner-ad {
    position: fixed;
    width: 80%;
    left: 50%;
    transform: translate(-50%);
    margin: 30rpx auto;
    background-color: rgba(0,0,0,0.6);
    z-index: 10000;
}

.popup-blacklist {
    width: 500rpx;
    height: 712rpx;
    background: url("https://txcdn.ylll111.xyz/hzttk/success.png");
    background-size: 100% 100%;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    padding-top: 210rpx;
    box-sizing: border-box;
}

.popup-blacklist image {
    width: 204rpx;
    height: 200rpx;
    display: block;
    margin: 0 auto 150rpx;
}

.popup-blacklist .btn {
    width: 320rpx;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    border-radius: 60rpx;
    color: #fff;
    font-size: 32rpx;
    background: #e14e4c;
    margin: 0 auto;
}