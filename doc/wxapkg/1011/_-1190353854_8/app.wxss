.clearfix::after {
    content: "";
    display: block;
    clear: both;
}

.fl {
    float: left;
}

.fr {
    float: right;
}

.tc {
    text-align: center;
}

.width33 {
    width: 33.333%;
}

.width50 {
    width: 50%;
}

.width25 {
    width: 25%;
}

.hide {
    display: none;
}

.show {
    display: block;
}

.line1 {
    overflow: hidden;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
    word-break: break-all;
}

.line2 {
    overflow: hidden;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    word-break: break-all;
}

.line3 {
    overflow: hidden;
    display: -webkit-box;
    -webkit-line-clamp: 3;
    -webkit-box-orient: vertical;
    word-break: break-all;
}

.hover-class {
    animation-name: scale;
    animation-duration: .3s,.3s,.3s;
    animation-delay: 0,0,0;
}

@-webkit-keyframes scale {
    0% {
        transform: scale(1);
    }

    50% {
        transform: scale(.8);
    }

    100% {
        transform: scale(1);
    }
}

@keyframes scale {
    0% {
        transform: scale(1);
    }

    50% {
        transform: scale(.8);
    }

    100% {
        transform: scale(1);
    }
}

page {
    background-image: url('data:image/png;base64,');
    background-repeat: repeat;
}

page::after {
    content: "";
    position: fixed;
    left: 0;
    top: 0;
    z-index: -1;
    width: 750rpx;
    height: 90rpx;
    background-image: url(data:image/png;base64,);
    background-repeat: no-repeat;
    background-position: 0 0;
}

#loading {
    background-color: #ede7e0;
    height: 100%;
    width: 100%;
    position: fixed;
    z-index: 10000;
    margin-top: 0px;
    top: 0px;
}

#loading-center {
    width: 100%;
    height: 100%;
    position: relative;
}

#loading-center-absolute {
    position: absolute;
    left: 50%;
    top: 50%;
    height: 300rpx;
    width: 300rpx;
    margin-top: -150rpx;
    margin-left: -100rpx;
}

#loading .object {
    width: 40rpx;
    height: 40rpx;
    background-color: #999;
    float: left;
    margin-right: 40rpx;
    margin-top: 130rpx;
    border-radius: 50% 50% 50% 50%;
}

#object_one {
    animation: object_one 1.5s infinite;
}

#object_two {
    animation: object_two 1.5s infinite;
    animation-delay: 0.25s;
}

#object_three {
    animation: object_three 1.5s infinite;
    animation-delay: 0.5s;
}

@-webkit-keyframes object_one {
    75% {
        transform: scale(0);
    }
}

@keyframes object_one {
    75% {
        transform: scale(0);
    }
}

@-webkit-keyframes object_two {
    75% {
        transform: scale(0);
    }
}

@keyframes object_two {
    75% {
        transform: scale(0);
    }
}

@-webkit-keyframes object_three {
    75% {
        transform: scale(0);
    }
}

@keyframes object_three {
    75% {
        transform: scale(0);
    }
}